# Environment Configuration
NODE_ENV=development
PORT=5001

# Database
DB_PATH=./database.sqlite

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# File Upload
MAX_FILE_SIZE=50000000
UPLOAD_PATH=./uploads

# CORS
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

